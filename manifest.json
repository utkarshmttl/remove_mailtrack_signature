{
  "name": "Remove MT signature",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "tabs","<all_urls>"
    ],
    "content_scripts": [
        {
        "matches": [
            "http://mail.google.com/*",
            "https://mail.google.com/*"
            ],
        "js": ["main.js"],
        "run_at": "document_end"         // pay attention to this line
        }
    ], 
  "manifest_version": 2
}
